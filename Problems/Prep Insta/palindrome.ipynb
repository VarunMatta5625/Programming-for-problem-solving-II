   "source": [
    "def num_reverser(a):\n",
    "    rev = 0\n",
    "    while(a!=0):\n",
    "        rev = (rev*10) + (a%10)\n",
    "        a = a//10\n",
    "    return rev "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Not Palindrome\n"
     ]
    }
   ],
   "source": [
    "n = int(input(\"Enter a number to check if it is palindrome\"))\n",
    "\n",
    "if(n == num_reverser(n)):\n",
    "    print(\"Palindrome\")\n",
    "else:\n",
    "    print(\"Not Palindrome\")"
   ]
